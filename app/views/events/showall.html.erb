<p id="notice"><%= notice %></p>

<h1>Current Events: </h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= Event.where(id: event.event_id).last.event_name %></td>
        <td><%= link_to 'Show', event %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>

<%= link_to 'Home', root_path %>

